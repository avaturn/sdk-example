{
  "name": "avaturn-sdk-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "./examples/*"
  ],
  "scripts": {
    "dev": "concurrently \"yarn start:base\" \"yarn start:react\" \"yarn start:vanilla\"",
    "build": "vite build",
    "preview": "vite preview",
    "start:base": "vite --port 3000",
    "start:react": "cd examples/react && vite --port 3001",
    "start:vanilla": "cd examples/vanilla && vite --port 3002"
  },
  "devDependencies": {
    "sass": "^1.69.5",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
